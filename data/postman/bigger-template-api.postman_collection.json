				{
					"name": "sign-in (cognito)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Get access token from response and set it as collection variable for future requests.\r",
									"const signUpResponse = pm.response.json();\r",
									"const accessToken = signUpResponse.AuthenticationResult.AccessToken;\r",
									"pm.collectionVariables.set(\"access_token\", accessToken);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "X-Amz-Target",
								"value": "AWSCognitoIdentityProviderService.InitiateAuth",
								"type": "text"
							},
							{
								"key": "Content-Type",
								"value": "application/x-amz-json-1.1",
								"type": "text"
							}
						],
						"body": {
